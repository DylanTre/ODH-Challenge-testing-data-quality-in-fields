{
  "number": [
    {
      "key_match": "num.*",
      "more_than": 10,
      "even": true
    },
    {
      "key_match": "num.*",
      "less_than": 10,
      "even": true
    }
  ],
  "string": {
    "key_match": ".*",
    "value_match": ".*t"
  },
  "date": [
    {
      "key_match": "date.*",
      "format": "dd/MM/yyyy HH:mm:ss",
      "day_of_week": "monday",
      "is_before": "27/11/2023",
      "is_after": "11/11/2023"
    },
    {
      "key_match": "datett.*",
      "format": "dd/MM/yyyy HH:mm:ss",
      "day_of_week": "monday",
      "is_before": "27/11/2023",
      "is_after": "11/11/2023"
    }
  ],
  "boolean": {
    "key_match": "bool",
    "expected": true
  },
  "list": {
    "key_match": "areas",
    ""
  },
  "object": {
    "key_match": "nestedObject",
    "validators": [
      {
        "string": {
          "key_match": ".*",
          "value_match": ".*t"
        }
      },
      {
        "object": {
          "key_match": "object",
          "validators": [
              "string",
              "number"
          ]
        }
      }
    ]
  }
}
